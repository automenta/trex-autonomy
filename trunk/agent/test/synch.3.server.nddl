#include "synch.nddl"

// Will always abort
ResetCircuitBreaker::Inactive{
  if test (start <= 6){
    eq(status, ABORTED);
  }
  else{
    eq(status, SUCCESS);
  }
}

ResetCircuitBreaker::Active{
  eq(duration, 1);
}

ResetCircuitBreaker rcb = new ResetCircuitBreaker(Internal);

fact(rcb.Inactive);

